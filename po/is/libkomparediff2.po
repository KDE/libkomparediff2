"qt>"

#: komparemodellist.cpp:319 komparemodellist.cpp:680
#, kde-format
msgid "Could not open a temporary file."
msgstr "Get ekki opnað bráðabirgðaskrá."

#: komparemodellist.cpp:367 komparemodellist.cpp:374
#, kde-format
msgid "<qt>Could not write to the temporary file <b>%1</b>, deleting it.</qt>"
msgstr "<qt>Gat ekki skrifað í bráðabirgðaskrána <b>%1</b>, eyði henni.</qt>"

#: komparemodellist.cpp:405
#, kde-format
msgid ""
"<qt>Could not create destination directory <b>%1</b>.\n"
"The file has not been saved.</qt>"
msgstr ""
"<qt>Gat ekki búið til móttökumöppu <b>%1</b>.\n"
"Ekki var hægt að vista skrána.</qt>"

#: komparemodellist.cpp:433
#, kde-format
msgid ""
"<qt>Could not upload the temporary file to the destination location <b>%1</"
"b>. The temporary file is still available under: <b>%2</b>. You can manually "
"copy it to the right place.</qt>"
msgstr ""
"<qt>Gat ekki sent bráðabirgðaskrána á áfangastaðinn <b>%1</b>. "
"Bráðabirgðaskráin er enn tiltæk í: <b>%2</b>. Þú getur afritað hana "
"handvirkt á rétta staðinn.</qt>"

